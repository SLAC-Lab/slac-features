  'title' => 'Menu Tricks #3: Automating Virtual Menu Position for Specific Content Types',
  'log' => '',
  'status' => 1,
  'comment' => 2,
  'promote' => 1,
  'sticky' => 0,
  'vuuid' => '7ef8c3e3-c750-44f5-880a-e0d6b14aa225',
  'type' => 'blog',
  'language' => 'und',
